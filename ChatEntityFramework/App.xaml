<Application x:Class="ChatEntityFramework.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ChatEntityFramework"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ContactWindow.xaml"/>
                <ResourceDictionary Source="MainChatInterfaceWindow.xaml"/>
                <ResourceDictionary Source="MessageBox.xaml"/>
                <ResourceDictionary Source="UsernameEnter.xaml"/>
                <ResourceDictionary Source="GroupName.xaml"/>

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

